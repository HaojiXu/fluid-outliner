    "dist-webpack"
  ],
  "scripts": {
    "test": "jest",
    "build-server": "tsc",
    "build-client": "webpack --config webpack.config.ts",
    "build": "yarn run build-server && yarn run build-client",
    "webpack-dev-server": "webpack-dev-server",
    "prettier": "prettier --write --trailing-comma ./**/*.ts",
