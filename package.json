    "@types/react-dom": "^16.0.6",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "jest-environment-node": "^22.0.0",
    "jest-puppeteer": "^3.0.1",
    "offline-plugin": "^5.0.5",
    "prettier": "^1.13.5",
    "puppeteer": "^1.5.0",
    "ts-loader": "^4.4.1",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
